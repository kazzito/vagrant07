
<VirtualHost *:80>
    DocumentRoot /var/www/html/janusApp/webroot
    ServerName januslocal

    ErrorLog logs/januslocal-error_log
    CustomLog logs/januslocal-access_log common

    <Directory "/var/www/html/janusApp">
        Options FollowSymLinks
        AllowOverride All
        Order allow,deny
        Allow from all
    </Directory>
</VirtualHost>